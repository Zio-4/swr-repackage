{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "camelcase": false,
    },
    "ignorePatterns": [
        "**/node_modules/**",
        "**/dist/**"
    ]
}
